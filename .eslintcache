gens da data\n  async function getPhotos(date = selectedDate) {\n    const newPhotos = await NasaServices.listPhotos(rover_name, date, page);\n    setSelectedDate(date);\n    setPhotos([...photos, ...newPhotos]);\n    setLoading(false);\n    setFetching(false);\n  }\n\n  // Adiciona mais fotos na lista\n  function morePhotos() {\n    setPage(page + 1);\n    setFetching(true);\n    getPhotos();\n  }\n\n  return (\n    <Page>\n      <header style={{display: 'flex', justifyContent: 'space-between', marginBottom: 15}}>\n        <Typography variant=\"h3\">Fotos</Typography>\n        <TextField\n          id=\"date\"\n          label=\"Data inicial\"\n          type=\"date\"\n          value={selectedDate}\n          onChange={(e) => changeDate(e.target.value)}\n          InputLabelProps={{\n            shrink: true,\n          }}\n        />\n      </header>\n      {!loading ? <RoverCard displayName={false} rover={rover} /> : null}\n      <Grid container spacing={3}>\n        {photos.map((item) => (\n          <Grid key={item.id} item xs={4}>\n            <Post data={item} />\n          </Grid>\n        ))}\n        <Grid key={0} item xs={12}>\n          {loading || fetching ? (\n            <LinearProgress color=\"secondary\" />\n          ) : (\n            <div style={{ display: \"flex\", justifyContent: \"center\" }}>\n              <Button\n                onClick={() => morePhotos()}\n                style={{ margin: \"auto\", width: 250, background: '#96281b' }}\n                size=\"large\"\n                variant=\"contained\"\n                color=\"secondary\"\n              >\n                Ver Mais\n                &nbsp;\n                <AddIcon/> \n              </Button>\n            </div>\n          )}\n        </Grid>\n      </Grid>\n    </Page>\n  );\n}\n\nexport default Rovers;\n","/Users/wendreo/Documentos/konduto/red_planet_client/src/components/calendar.js",["96"],"import React from \"react\";\nimport _ from \"lodash\";\n\nfunction Calendar